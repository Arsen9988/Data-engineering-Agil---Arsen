    "import requests\n",
    "import pandas as pd\n",
    "\n",
    "# URL till API-endpunkten\n",
    "url = \"https://pokeapi.co/api/v2/pokemon-species/pikachu\"\n",
    "\n",
    "# Gör en GET-förfrågan till API:et\n",
    "response = requests.get(url)\n",
    "\n",
    "data = response.json()\n",
    "df = pd.json_normalize(data)\n",
    "\n",
    "df[\"base_happiness\"]\n",
    "\n"
   ]
  }
